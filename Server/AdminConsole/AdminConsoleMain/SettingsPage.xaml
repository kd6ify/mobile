<Page x:Name="SettingsPage1" x:Class="FutureConcepts.Mobile.AdminConsole.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:v="clr-namespace:FutureConcepts.Mobile.AdminConsole"
    xmlns:dc="clr-namespace:FutureConcepts.Mobile.DataContract;assembly=MobileDataContract"
    Title="SettingsPage"
    Loaded="Page_Loaded" >
    <Page.Resources>
    </Page.Resources>
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal" >
                <TextBox Text="ServerUrl: " />
                <TextBox x:Name="serverUrl" Width="250" TextChanged="serverUrl_TextChanged" />
            </StackPanel>
            <Button x:Name="saveChanges" Click="saveChangesButton_Click" Height="50" Width="100" HorizontalAlignment="Left" >Save Changes</Button>
        </StackPanel>
    </Grid>
</Page>
